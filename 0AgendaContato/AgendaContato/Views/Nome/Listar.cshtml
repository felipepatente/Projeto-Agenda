<ul class="nav nav-pills pull-right">
    <li>@Html.ActionLink("Voltar", "Menu", "Email")</li>
</ul>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var nomes in ViewBag.Nomes)
        {
            <tr>
                <td>@nomes.Id</td>
                <td>@nomes.NomeContato</td>
            </tr>
        }
    </tbody>
</table>