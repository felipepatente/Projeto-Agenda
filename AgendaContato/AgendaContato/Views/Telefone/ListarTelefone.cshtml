@model IList<AgendaContato.Models.Telefone>

<ul class="nav nav-pills pull-right">
    <li>@Html.ActionLink("Voltar", "Menu", "Telefone")</li>
</ul>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Número</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var telefone in Model)
        {
            <tr>
                <td>@telefone.Id</td>
                <td>@telefone.NomeContato.NomeContato</td>
                <td>@telefone.Numero</td>
                <td>@Html.RouteLink("Excluir", "ExcluirTelefone", new { id = telefone.Id })</td>
            </tr>
        }
    </tbody>
</table>
