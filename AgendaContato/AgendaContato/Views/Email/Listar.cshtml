<ul class="nav nav-pills pull-right">
    <li>@Html.ActionLink("Voltar", "Menu", "Email")</li>
</ul>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>E-mail</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var email in ViewBag.Emails)
        {
            <tr>
                <td>@email.Id</td>
                <td>@email.NomeContato.NomeContato</td>
                <td>@email.EmailContato</td>
            </tr>
        }
    </tbody>
</table>